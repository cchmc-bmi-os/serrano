[run]
branch = True
source = serrano
omit =
    */admin.py
    */migrations/*
    serrano/mediatypes.py
    serrano/__init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    def __unicode__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplemented(Error)?
    if 0:
    if __name__ == .__main__.:
